<script language="JavaScript" src="http://static.earthcamcdn.com/newyears/js/jquery.js" type="text/javascript"></script>

<!-- Android Smart App Banner-->
<!--<script src="http://static.earthcamcdn.com/newyears/js/jquery.smartbanner.js"></script>-->

<script language="JavaScript" type="text/javascript">
var $j = jQuery.noConflict();
var year = "2016";
var month = '1';
var day = '1';
var hour = 0;
var min = 0;
var sec = 0;
var msec = 0;
var tz = -5;
var lab = 'tzcd';
var timer;

window.onload = start;

jQuery(function(){
/*
	jQuery.smartbanner({
	  title: 'Webcams',
	  author: 'EarthCam',
	  daysHidden: 0,
	  daysReminder: 0,
	  icon: '',
	});
*/
});

function start() {
    displayTZCountDown(setTZCountDown(year, month, day, hour, min, sec, msec, tz), lab);
}

function setTZCountDown(year, month, day, hour, min, sec, msec, tz) {
    var toDate = new Date();
    toDate.setFullYear(year);
    toDate.setMonth(month - 1);
    toDate.setDate(day);
    toDate.setHours(hour);
    toDate.setMinutes(min - (tz * 60));
    toDate.setSeconds(sec);
    toDate.setMilliseconds(msec);
    var fromDate = new Date();
    fromDate.setMinutes(fromDate.getMinutes() + fromDate.getTimezoneOffset());
    var diffDate = new Date(0);
    diffDate.setMilliseconds(toDate - fromDate);
    return diffDate.valueOf();
}
function displayTZCountDown(countdown, tzcd) {
    if (countdown < 0) document.getElementById(tzcd).innerHTML = "Happy New Year Times Square!";
    else {
        var msecs = countdown % 1000;
        if (msecs < 100) msecs = '0' + msecs;
        if (msecs < 10) msecs = '0' + msecs;
        countdown_s = Math.floor(countdown / 1000);
        var secs = countdown_s % 60;
        if (secs < 10) secs = '0' + secs;
        var countdown1 = (countdown_s - secs) / 60;
        var mins = countdown1 % 60;
        if (mins < 10) mins = '0' + mins;
        countdown1 = (countdown1 - mins) / 60;
        var hours = countdown1 % 24;
        if (hours < 10) hours = '0' + hours;
        var days = (countdown1 - hours) / 24;
        if (days < 10) days = '0' + days;
        document.getElementById(tzcd).innerHTML = days + " day" + (days == 1 ? '' : 's') + ' + ' + hours + 'h : ' + mins + 'm : ' + secs + 's' + "<br/>";
        document.getElementById(tzcd).innerHTML = days + ":" + hours + ':' + mins + ':' + secs + ':' + msecs;
        timer = setTimeout('start();', 48);
    }
}

</script>

</div>
<br />
<!--/content -->
</div>
</body>
</html>