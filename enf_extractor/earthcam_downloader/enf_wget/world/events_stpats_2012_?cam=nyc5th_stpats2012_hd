function isBrowserIE()
{
	var browser=navigator.appName;
	var b_version=navigator.appVersion;
	var version=parseFloat(b_version);
	
	if ( browser=="Microsoft Internet Explorer" )
		return(1);
	else
		return(0);
}


//make adjustments on the fly depending on what mode the player is in
function adjustTemplate(tToggle)
{
	//alert(tToggle);
	// make sure the ads aren't playing
	//if(thisMovie('movie').isSpotXPlaying())
		//return(false);

	if (tToggle == "hdon") // hd mode
	{		
		// experimental
		$('#leftNavCell').hide(1, function() {
			$('#topNavBar').fadeIn("fast");

		});
		
		$("#camContent").css("width",'960px');
		$("#ecnetworkcam").css("width",'960px');
		$("#flashContainer").css("width",'960px');
		$("#ECNglobalContainer").css("margin-left",'0px');
		$("#ECNglobalContainer").css("margin-top",'0px');
		$("#camContent").css("margin", '0 0 0 0');
	}
	else // sd mode
	{
		$("#camContent").css("width",'740px');
		$("#ecnetworkcam").css("width",'740px');
		$("#flashContainer").css("width",'740px');
		$("#ECNglobalContainer").css("margin-left",'25px');
		$("#ECNglobalContainer").css("margin-top",'15px');
		$("#camContent").css("margin", '15px 0px 0px 25px');
		
		$('#topNavBar').fadeOut(0, function() {
			$('#leftNavCell').fadeIn("fast", function() {});
		});
	}
}

function Pause() {
	timer = setTimeout("endpause()",1000);
	return false;
}

function endpause() {
	return false;
}




function setDivHeight(containerDiv,oDiv,numHeight,numWidth)
{
	containerDiv = "ecnetworkcam";
	oDiv = "movie";
	
	//divC = document.getElementById(containerDiv);
	//divC.style.height = numHeight+'px';
	//alert(numWidth+"x"+numHeight);
	
	if (ECN_player_version == "2")
	{
		
		//var numHeight
		
		$("#movie").css("height",numHeight+'px');
		$("#ecnetworkcam").css("height",numHeight+'px');
		$("#ECNglobalContainer").css("height",numHeight+'px');

		if (numWidth == undefined)
			numWidth = "740";

		//alert(numWidth);

		if (numWidth < 800)
			adjustTemplate('hdoff');
		else 
			adjustTemplate('hdon');
			
		//Pause();
		
		$("#movie").css("width",numWidth+'px');
		$("#ecnetworkcam").css("width",numWidth+'px');
		$("#flashContainer").css("width",numWidth+'px');
		$("#ECNglobalContainer").css("width",numWidth+'px');
		
	}
	else
	{
		// old method
		divObject = document.getElementById(oDiv);
		divObject.style.height = numHeight+'px';
	}
}

//functions for flash, must be inside the body tag (is this true??)
function thisMovie(movieName)
{
    if (navigator.appName.indexOf("Microsoft") != -1)
    {
        return window[movieName]
    }
    else
    {
        return document[movieName]
    }
}

function selectCamera(camName)
{
	//console.log("selectCamera function call");
	// hastag/history system
	//document.location.hash = "#!/" + camName;
	//History.pushState(null,null,"?cam="+camName)
	History.replaceState(null,null,"?cam="+camName)
	
	//return(1);
	currentName					= camName;
	//console.log(currentName);
	
	$j(".thumb_cam_switcher").removeClass("camSelectionOver").addClass("camSelectionOut")
	$j(".pageThumbnailContainer").removeClass("pageThumbnailContainer_on").addClass("pageThumbnailContainer_off");
	$j(".thumb_cam_text").removeClass("cam_text_on").addClass("cam_text_off");
	$j("#more_cams_img").removeClass("more_cams_img_on").addClass("more_cams_img_off");
	
	var current_div	= $j("#cam_"+currentName);
	current_div.removeClass("camSelectionOut").addClass("camSelectionOver")
	current_div.find(".pageThumbnailContainer").removeClass("pageThumbnailContainer_off").addClass("pageThumbnailContainer_on");
	current_div.find(".thumb_cam_text").removeClass("cam_text_off").addClass("cam_text_on");
	
	
	//alert(typeof js_bg_header);
	if (typeof js_bg_header != undefined)
	{
		var background_image	= js_bg_header[camName][0].bg;
		var header_image		= js_bg_header[camName][0].header;
		
		//console.log(background_image);
		
		if (background_image.length > 0)
			$j("body").css("background-image","url("+background_image+")")
		
		if (header_image.length > 0)
			$j("#header_image").attr("src",header_image)
	}
}

function switchCams(_cam, _extras)
{
	//console.log("switchCams function");
	$j('#cam_thumbs_dropdown').fadeOut("fast");
	thumbs_selector_dropdown_visible	= false;
	
	//alert("switchCams");
	if (js_basic_player)
	{
		_startPlayer_basic(_cam);
		return(true);
	}
	
	document.getElementById('movie').selectCamera(_cam,_extras); //selectCamera(_cam, _extras);	
}


