id_livepath%22%3A%22%5C%2Ffecnetwork%5C%2F4367.flv%22%2C%22cdn_status%22%3A%22false%22%2C%22archivepath%22%3A%22MP4%3A%5C%2Fevents%5C%2Fstpatricksday%5C%2F2012%5C%2Fstreetcam.mp4%22%2C%22timelapsepath%22%3A%22%5C%2Farchives5%5C%2Ftimelapse%5C%2Ffridays5th%5C%2Ftimelapse.mp4%22%2C%22windowsmediapath%22%3A%22%22%2C%22hofpath%22%3A%22newyork%5C%2Fnyc5th%5C%2F%22%2C%22logolink%22%3A%22%22%2C%22logopath%22%3A%22%5C%2Fcams%5C%2Fnewyork%5C%2Ftimessquare%5C%2Fimages%5C%2Ffridayslogo.swf%22%2C%22livestreamingbuffer%22%3A%225%22%2C%22archivebuffer%22%3A%225%22%2C%22timelapsebuffer%22%3A%225%22%2C%22name%22%3A%22A+Great+Irish+Day%22%2C%22group%22%3A%225th+Ave+%40+48th+St.%22%2C%22location%22%3A%22New+York%2C+New+York%22%2C%22description%22%3A%22%3Cspan+class%3D%27desc%27%3EEarthCam%27s+roaming+HD+street-level+cam+is+the+perfect+way+to+virtually+join+the+celebration%21+See+the+massive+crowds+gathered+on+the+sidelines+and+enjoy+the+colorful+bands%2C+floats+and+marchers%21%3C%5C%2Fspan%3E%22%2C%22archiveStartTime%22%3A%2250%22%7D%7D%7D";
		flashvars.json2			= "";
		flashvars.path			= "/cams/../events/stpats/2012/";
		flashvars.needWaterTemp = "false";
		flashvars.showplaceholder	= "false";
		flashvars.ecnBlack		= "false";
		flashvars.streamTimeOut	= '30';		flashvars.thumbLabel				= '';
		flashvars.thumbLabelRight			= '';
		flashvars.thumbLabel2				= '';
		flashvars.eventMode					= ''; //expands the player downward for 2 rows of cams, and world tour if needed
		flashvars.playerPathState			= "com";
		
				
		
		flashvars.startOnWorldTour			= false; //Start the player on the world tour? (true,false)
		flashvars.displayWorldTourGoogleAds = true;  //Show google ads on world tour button?(true,false)
		/*
		1460127335		*/
		
		var full_url = "http://www.earthcam.com/swf/cam_player_v2/ecnPlayer.swf?20160316";
		//var full_url = "/swf/cam_player_v2/ecnPlayer.swf?20160316";
		//var full_url = "http://cdn1.earthcam.com/earthcam/swf/cam_player_v2/ecnPlayer.swf?20160316";

		swfobject.embedSWF(full_url, "ecnetworkcam", "960", "450", "10","/swf/expressinstall.swf", flashvars, swfobject_params, attributes); // 9.0.115
		
				
	}
}

