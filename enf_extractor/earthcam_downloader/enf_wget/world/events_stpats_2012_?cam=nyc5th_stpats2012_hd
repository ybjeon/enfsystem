<script language="JavaScript" type="text/javascript">

// document.ready function
$(function(){
	$j("#smellovision").click(function(){
		//alert("April Fools!");
		clicksound.playclip();
		
		$j('#aprilfools').show();
		$j('#aprilfools').delay(5000).fadeOut('slow');
	});
	
	
	$j('#smellbutton').mouseover(function() { 
           $j(this).attr("src", "/events/april/baconcam/images/SmellThis2_100x99.png");
	}).mouseout(function() { 
           $j(this).attr("src", "/events/april/baconcam/images/SmellThis1_100x99.png");
    });
	
	/*$j("#smellovision").mouseover(function(){
		$j("#smellbutton").
	})
	.mouseout(function(){
	
	})*/
	
	
	$('#flashContainer').show();
	
	// init the player
		/*
			HISTORY JS implementation
		*/
		var History = window.History;
		var State;
		//var State = History.getState();
		
		var hash_cam = loadCameraViaHash();
		if (hash_cam == false)
		{
			State = History.getState();
		}
		
		// listen for history change events
		History.Adapter.bind(window,'statechange',function(){ // Note: We are using statechange instead of popstate
			State = History.getState(); // Note: We are using History.getState() instead of event.state
			var hash_cam = State.hash.match(/cam=(.*)(&|$)/i);
			//selectCamera(hash_cam[1]);
			//switchCams(hash_cam[1]);
		});
		// ------------------------------------------
	
		//alert(js_basic_player);
		if (js_basic_player)
			_startPlayer_basic('nyc5th_stpats2012_hd');
		else
			_startPlayer(); /*document.getElementById('helplearn').style.visibility = 'visible';*/
			
		//adjustTemplate('hdon');

	// top nav dropdown
	//var nav_overlay = $j('#top_nav_dropdown');
	$j('#top_nav_dropdown').mouseleave(function(){
		topnavHide(800);
		top_nav_dropdown_visible = false;
	});
	$j('#top_nav_dropdown').mouseenter(function(){
		$j('#top_nav_dropdown').clearQueue();
		top_nav_dropdown_visible = true;
	});
	
	
	$j('#cam_thumbs_dropdown').mouseleave(function(){
		//console.log("#cam_thumbs_dropdown mouseleave event");
		thumbSelectorHide(800);
		thumbs_selector_dropdown_visible = false;
	});
	$j('#cam_thumbs_dropdown').mouseenter(function(){
		$j('#cam_thumbs_dropdown').clearQueue();
		thumbs_selector_dropdown_visible = true;
	});
	
	
	$j('.thumb_cam_switcher').mouseover(function(){
		if ("cam_"+currentName != $j(this).attr('id'))
		{
			$j(this).removeClass("camSelectionOut").addClass("camSelectionOver")
			$j(this).find(".pageThumbnailContainer").removeClass("pageThumbnailContainer_off").addClass("pageThumbnailContainer_on");
			$j(this).find(".thumb_cam_text").removeClass("cam_text_off").addClass("cam_text_on");
		}
	});
	
	$j('.thumb_cam_switcher').mouseout(function(){
		if ("cam_"+currentName != $j(this).attr('id'))
		{
			$j(this).removeClass("camSelectionOver").addClass("camSelectionOut");
			$j(this).find(".pageThumbnailContainer").removeClass("pageThumbnailContainer_on").addClass("pageThumbnailContainer_off");
			$j(this).find(".thumb_cam_text").removeClass("cam_text_on").addClass("cam_text_off");
		}
	});
	
	$j('#more_cams_img').mouseover(function(){
		if (!thumbs_selector_dropdown_visible)
			$j("#more_cams_img").removeClass("more_cams_img_off").addClass("more_cams_img_on");
	});
	
	$j('#more_cams_img').mouseout(function(){
		if (!thumbs_selector_dropdown_visible)
			$j("#more_cams_img").removeClass("more_cams_img_on").addClass("more_cams_img_off");
	});
	
	
	$j('#more_cams_img').click(function(){
		thumbSelectorPopup();
	});
	
	
	/*
	$j('#top_nav_dropdown').hover(function(){
		$j('#moreArrow_ecn_nav').attr('src', 'http://static.earthcamcdn.com/images/search/MoreArrow-Hover.gif');
	},
	function(){
		$j('#moreArrow_ecn_nav').attr('src', 'http://static.earthcamcdn.com/images/search/MoreArrow-On.gif');
	});
	*/

	//$('#superContentTable').css("visibility","visible");
	// experimental overlay dragging
	//$("#ecnOverlay").draggable();
	
	// testing out hash stuff
	// Bind an event to window.onhashchange that, when the hash changes, gets the
	// hash and adds the class "selected" to any matching nav link.

/*	
	$j(window).hashchange( function(){
		var _hash_cam	= loadCameraViaHash();
		switchCams(_hash_cam);
	})
	*/

	
	/*
		IF FLASH IS NOT INSTALLED
	*/
	//var flash_version = GetSwfVer();
	var _flash_installed = ((typeof navigator.plugins != "undefined" && typeof navigator.plugins["Shockwave Flash"] == "object") || (window.ActiveXObject && (new ActiveXObject("ShockwaveFlash.ShockwaveFlash")) != false));
	
	if (!_flash_installed)
	{
		var install_flash_message = 
		'<div>'+
			'<br /><br /><br /><br />'+
			'<h4>EarthCam Network content requires the latest version of the Adobe Flash Player</h4>'+
			'<br />'+
			'<div><a target="_blank"href="http://www.adobe.com/go/getflashplayer"><img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" /></a></div>'+
		'</div>';
			
		$j("#ecnetworkcam").html(install_flash_message);
	}

	
});


