var currentHDiterator = 1;
function switchTSCams()
{
	//alert(currentHDiterator);
	var newTSCam = "";
	// javascript: selectCamera(lennon_hd)
	// lennon_hd,fridays,tsone_str
	var tsHDCams = new Array("lennon_hd","fridays","tstwo_hd"); // "tsone_str",
	
	newTSCam = tsHDCams[currentHDiterator];
	
	//selectCamera(newTSCam);
	document.getElementById('movie').selectCamera(newTSCam);
	currentName = newTSCam;

	if (currentHDiterator == 2)
		currentHDiterator = 0;
	else	
		currentHDiterator++;

}

function getQueryVariable(variable){var query=window.location.search.substring(1);var vars=query.split("&");for(var i=0;i<vars.length;i++){var pair=vars[i].split("=");if(pair[0]==variable){return pair[1];}}}


/********************/


function loadCameraViaHash()
{
	var pathname = $j(location).attr('pathname'); 
	
	if (location.hash.match(/#\?cam=(.*)(&|$)/i))
	{
		//1 var hash = location.hash.replace('#', '');
		//1 var camera = hash.substr(2)
		
		var _camera = location.hash.match(/#\?cam=(.*)(&|$)/i);
		
		return(_camera[1]);
	}

	//failsafe if hash isn't found
	return(false);
}


function topnavHide(delay){
	$j('#top_nav_dropdown').delay(delay).fadeOut("fast");
}

function topNavPopup()
{
	if (top_nav_dropdown_visible)
	{
		top_nav_dropdown_visible = false;
		$j('#top_nav_dropdown').fadeOut("fast");
	}
	else
	{
		top_nav_dropdown_visible = true;
		$j('#top_nav_dropdown').fadeIn("fast");
	}
}


function thumbSelectorHide(delay){
	$j('#cam_thumbs_dropdown').delay(delay).fadeOut("fast", function() {
		$j("#more_cams_img").removeClass("more_cams_img_on").addClass("more_cams_img_off");
	});
}

function thumbSelectorPopup()
{
	if (thumbs_selector_dropdown_visible) // hiding it
	{
		thumbs_selector_dropdown_visible = false;
		$j("#more_cams_img").removeClass("more_cams_img_on").addClass("more_cams_img_off");
		$j('#cam_thumbs_dropdown').fadeOut("fast");
	}
	else // showing it
	{
		gaRecordInternal(-1, 'ECN_thumbdropdown', '/internal/ecn/camthumbdropdown');
		
		thumbs_selector_dropdown_visible = true;
		$j("#more_cams_img").removeClass("more_cams_img_off").addClass("more_cams_img_on");
		$j('#cam_thumbs_dropdown').fadeIn("fast");
	}
}

function _startPlayer_basic(_cam)
{
	//alert(_cam);
	//alert(json_basic.cam[_cam].hd);
	//alert( json_basic['cam'][_cam]['hd'] );
	
	// determine if hash tag history is present
	//loadCameraViaHash();
	var camData = json_basic['cam'][_cam];
	var basic_html = "";
	var flashContainer = $("#flashContainer");
	var sWidth;
	var sHeight;

	var loading = 
	'<div style="width: 960px; height: 540px;">'+
	'<p style="height: 140px; text-align:center; font-size: 1px;">&nbsp;</p>'+
	'Loading Player'+
	'<br /><br />'+
	'<img style="border: 1px solid black;" src="http://static.earthcamcdn.com/mapsearch_google/images/loading_screen.gif" width="50" alt="loading" />'+
	'</div>';
	
	//alert("basic player init " + _cam + " | live type " + camData.live_type);

	// special cases
	if (camData.group_id =="statueofliberty" || camData.group_id =="warhol" || camData.group_id =="fishbowl_2014" || camData.group_id =="aprilfools2014" || camData.group_id =="popefrancisholymass" || camData.group_id =="sandyhook") // && typeof getCookie("warholQR") != "undefined"))
	{	
		//alert("exception");
		
		if (_cam == "liberty_torch" )
		{
			sWidth			= "960";
			sHeight			= "540";
			basic_html	= '<div id="ecvrDIV" style="width:'+sWidth+'; height:'+sHeight+'px;"></div>';
			$("#ECNglobalContainer").css("height",sHeight+'px');
			flashContainer.html(loading);
			flashContainer.html(basic_html);
			ecvr.init();
			return(1);
		}

		// all the rest
		if (camData.live_type == "mega_hd" )
		{
			sWidth			= "960";
			sHeight			= "540";
			var sPath		= camData.livestreamingpath;//livestreamingpath

			if (camData.cdn_status == "true")
				sFile			= sPath.substr(8) + ".flv";
			else
				sFile			= sPath.substr(12); //  /fecnetwork/libertyHD1.flv
			
			//console.log(sPath);
			//console.log(sFile);
			
			var frameSrc	= '/cams/includes/player_viewer_f.php?inet='+camData.inet+'&type=h264&vid='+sFile+'&w='+sWidth+'&h='+sHeight;
			basic_html 		= '<iframe width="'+sWidth+'" height="'+sHeight+'" frameborder="0" marginheight="0" marginwidth="0" scrolling="no" src="'+frameSrc+'"></iframe>';
		}
		else
		{
			sWidth			= "960";
			sHeight			= "540";
			var timestamp	= new Date().getTime();
			var iPath;
			
			// bandaid for the liberty popart cam
			if (_cam == "liberty_popart")
				iPath		= "http://www.earthcam.com/cams/includes/image.php?img=bc8cfbf74da8fdd0156a05eaf7edfa82" + "&time=" + timestamp;
			else
				iPath		= camData.imagedomain + camData.livepath + "?"+ timestamp;

			basic_html		= '<div id="basic_loading"><img src="'+iPath+'" width="'+sWidth+'" height="'+sHeight+'"  alt="" title="" /></div>';
		}
	}
	else // regular case
	{
		sWidth			= "960";
		sHeight			= "540";
		var timestamp	= new Date().getTime();
		//var iPath		= camData.imagedomain + camData.livepath + "?"+ timestamp;
		//var iPath		= "http://archives.earthcam.com/archives5/camshot/" + camData.inet + ".jpg" + "?"+ timestamp;
		var iPath		= "http://www.earthcam.com/cams/includes/image.php?img=" + camData.inet + "&amp;"+ timestamp;
		basic_html		= '<div id="basic_loading"><img src="'+iPath+'" width="'+sWidth+'" height="'+sHeight+'"  alt="" title="" /></div>';
	}
	
	//alert(loading);
	$("#ECNglobalContainer").css("height",sHeight+'px');
	//flashContainer.html(loading);
	flashContainer.html(basic_html);
	
		
	
	return(1);
}

function _startPlayer()
{
	//alert ("player init");

	/********************/
	// global vars for swfobject sections
	var swfobject_params = {}
	swfobject_params.bgcolor ="#000000"; //,
	swfobject_params.wmode = "transparent";
	swfobject_params.allowFullScreen = "true";
	// swfobject_params.allowScriptAccess = "sameDomain";
	swfobject_params.allowScriptAccess = "always";
	
	var attributes = {}
	  attributes.id		= "movie",
	  attributes.name	= "movie";

	var flashvars = {};
	/********************/
	
	if (ECN_player_version == "2")
	{
		swfobject_params.bgcolor ="#FFFFFF";
		swfobject_params.wmode = "transparent";
		swfobject_params.scale = "noborder";
		
		
		flashvars.hidebanner	= "false";
		
		var js_defaultCam = getQueryVariable('cam');
		if ( js_defaultCam == undefined) { js_defaultCam = 'nyc5th_stpats2012_hd'; }
		flashvars.cam			= js_defaultCam;
		
		// determine if hash tag history fallback is present, for IE <=9 and compat mode
		var hash_cam = loadCameraViaHash();
		if (hash_cam !== false)
			flashvars.cam			= hash_cam;
		else
			flashvars.cam			= "nyc5th_stpats2012_hd";
		
		//flashvars.timestamp		= "1460127335";	
		flashvars.json			= "%7B%22cam%22%3A%7B%22nyc5th_stpats2012_str%22%3A%7B%22mapmode%22%3A%22staticmaplite%22%2C%22mappath%22%3A%22%22%2C%22live_tab_text%22%3A%22LIVE%22%2C%22reservearchives%22%3A%22false%22%2C%22custom_background%22%3A%22%22%2C%22custom_header%22%3A%22%22%2C%22offlineMessage%22%3A%22%22%2C%22offlineMessageSub%22%3A%22%22%2C%22showOfflineMessage%22%3A%22true%22%2C%22worldtour_path%22%3A%22rtmp%3A%5C%2F%5C%2Fvideo3.earthcam.com%5C%2Ffecnetwork%5C%2Ffridays_5th.flv%22%2C%22worldtour%22%3A%22false%22%2C%22worldtour_type%22%3A%22updatingimage%22%2C%22worldtour_delay%22%3A%2215000%22%2C%22maintainAspectRatio%22%3A%221%22%2C%22bestofdomain%22%3A%22rtmp%3A%5C%2F%5C%2Fvideo2.earthcam.com%5C%2Farchives%22%2C%22bestofpath%22%3A%22%22%2C%22bestofbuffer%22%3A%225%22%2C%22bestofstarttime%22%3A%22%22%2C%22goto%22%3A%22live%22%2C%22defaulttab%22%3A%22live%22%2C%22image_rotate%22%3A%220%22%2C%22rotate%22%3A%220%22%2C%22offlineimages%22%3A%22stpats_offline1.jpg%22%2C%22is3D%22%3A%22false%22%2C%22hideIconMap%22%3A%22false%22%2C%22archiveVideoSelector%22%3A%22false%22%2C%22thumbnail%22%3A%227412cdcc8b96b1891316259b98537f6f%22%2C%22page_thumbnail%22%3A%220da49185280fd5ad8de5320c7c3145b0%22%2C%22url%22%3A%22%22%2C%22weather%22%3Anull%2C%22hidden%22%3A%22false%22%2C%22hd%22%3A%22mega_hd%22%2C%22overlay%22%3A%22true%22%2C%22overlay_archives%22%3A%22true%22%2C%22overlay_timelapse%22%3A%22true%22%2C%22id%22%3A%22e79ff7c4fc3a12386a3b27d43379b8fe%22%2C%22inet%22%3A%222b3df1d118f9bcde5dfd351a32cbe86e%22%2C%22wmv_for%22%3A%22live%22%2C%22longbanner%22%3A%22%22%2C%22placeholderpath%22%3A%22%22%2C%22coords%22%3A%2240.75779830634708%2C-73.97795319557190%22%2C%22map_lat%22%3A%2240.75779830634708%22%2C%22map_long%22%3A%22-73.97795319557190%22%2C%22cam_counter%22%3A1%2C%22cam_state%22%3A1%2C%22cam_name%22%3A%22nyc5th_stpats2012_str%22%2C%22group_id%22%3A%22stpats_2012%22%2C%22group_order%22%3A%221%22%2C%22default_cam%22%3A%22%22%2C%22xml_path%22%3A%22newyork%5C%2Ffifthave%22%2C%22live