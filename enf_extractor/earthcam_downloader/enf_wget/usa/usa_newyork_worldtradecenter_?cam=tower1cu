function setupMobileThumbs()
{
	var timeZonesWithCams	= [];
	//var tz_offset, count = 1;
	var count				= 1;
	var _thumbnail_html		= "";	
	foundWorld				= false;
	// <div class="mobile_thumbs" style="margin: 0px auto; text-align: center; width: 960px;">';
	
	if ("newyears" == js_event_mode)
	{
		// create array of timezone offsets
		for(var offset in json_base['world_cams']){
			timeZonesWithCams.push(offset);
		}
		
		// add world cams to json_base
		for(x = 0; x < timeZonesWithCams.length; x++)
		{
			var tz_offset = timeZonesWithCams[x];
			
			for(var i in json_base['world_cams'][tz_offset])
			{			
				var _cam_name = json_base['world_cams'][tz_offset][i].cam_name;
				//console.log(x, tz_offset, i );
				//console.log(_cam_name);

				json_base['world_cams'][tz_offset][i].cam_counter = 100 + count;
				json_base['world_cams'][tz_offset][i].cam_type = "worldcam";
				json_base['cam'][_cam_name] = json_base['world_cams'][tz_offset][i];
				count++;
			}
		}
		
		//generate thumbnail html
		for(var x in json_base['cam'])
		{
			var _cam_data = json_base['cam'][x];
			//var cam_name = json_base['cam'][cam_name].cam_name;
			var cam_name = _cam_data.cam_name;
			
			if (cam_name == "world_cams_spacer")
				continue;
					
			if ("worldcam" == _cam_data.cam_type && foundWorld == false)
			{
				//console.warn("found world");
				_thumbnail_html += '<div style="clear: left; height: 10px;"></div>';
				foundWorld = true;
			}
			
			/*_thumbnail_html +=
				'<div data-cam=\''+cam_name+'\' class="thumb_basic thumb normal" '+
					'style="float: left; margin-right: 20px; cursor:pointer;">'+
					'<p><img src="http://static.earthcamcdn.com'+_cam_data.thumbimage2+'" /></p>'+
					'<p style="font-size: 11px; font-family: Verdana;">'+_cam_data.camtext+'</p>'+
				'</div>';*/
			
		}
	}
	
	jQuery('.selectorArea').addClass('small');
	jQuery('.selectorArea').css( {"display":"block","visibility":"visible"} );
	jQuery('#ecnPlayer .cameraSelector ').css("display","block");

	
	$cameraSelector = jQuery(".cameraSelector");
	_cameraSelector = new earthcam.ECNCameraSelector($cameraSelector);
	var _width = 960;
	
	cameraConfig = {};
		cameraConfig.cams = json_base['cam'];
		cameraConfig.related = {};
		cameraConfig.playerSize = _width;
		cameraConfig.enableTour = false;
	_cameraSelector.init(cameraConfig);
	
	_cameraSelector.highlightSelected(null);
		
		
	/*	
	_thumbnail_html += '<div style="clear: left;"></div>';
	jQuery('.selectorArea').css("display","block").css("visibility","visible").css("height","160px").css("overflow-y","scroll");
	jQuery('.selectorArea').html(_thumbnail_html);
	

		//jQuery('#ecnPlayer .selectorArea').addClass('small').css("visibility","visible").css("height","80px");;
		//jQuery('#ecnPlayer .cameraSelector').css("display","block");
		//jQuery('#ecnPlayer .thumbs').html(_thumbnail_html);

	
	$j(document).on('click', '.thumb_basic', function() {
		var camera = jQuery(this).data('cam');
		_startPlayer_basic(camera);
	});
	*/
	
	return true;
}

function _startPlayer_basic(_cam)
{
	var top_segment_content = '';
	var camData			= json_base['cam'][_cam];
	
	//console.warn(_cam);
	//console.warn(camData);
	//return false;
	
	var basic_html		= "";
	var flashContainer	= $j(".flashContainer");
	var sWidth;
	var sHeight;
	js_allowmobile		= camData.allowmobile;
	currentName			= _cam;
	var hofid			= camData.hofid;
	var cam_state		= camData.cam_state;
	var offlineimage	= camData.offlineimage;
	//console.log(camData);
	//console.log('hofid is '+hofid);
	
	/*
		Temporary until mobile integration is done on the ecnplayer class level
	*/
		if (template_mode == "myec") {
			History.replaceState(null,null,"");
		}
		else {
			History.replaceState(null,null,"?cam="+currentName);
		}
		
		loadCameraLikes(currentName,camData.likes);
		loadMapImage(camData.id,camData.map_lat,camData.map_long);
		loadPovCopy(camData.description);
		loadAffiliateLogo(camData.logopath, camData.logolink);
		
		earthcam.events.Event.dispatchEvent(new earthcam.events.ECNEvent(earthcam.events.ECNEvent.HOF_ID_CHANGE, {"hofid":hofid}));
		//hof.manual_loadHallOfFame();//hof_display_mode //handled by event
		//loadHallOfFame(hofid);
	
	
	$j('#ecnPlayer').css('min-height','650px');
	$j('#ecnPlayer').css('height','650px');
	
	var loading = 
	'<div style="width: 1000px; height: 540px;">'+
	'<p style="height: 140px; text-align:center; font-size: 1px;">&nbsp;</p>'+
	'Loading Player'+
	'<br /><br />'+
	'<img style="border: 1px solid black;" src="http://static.earthcamcdn.com/mapsearch_google/images/loading_screen.gif" width="50" alt="loading" />'+
	'</div>';

	var unix_timestamp = Math.floor(Date.now() / 1000);
	var seconds_week = 60*60*24*7; // 604,800
	
	if ( Math.abs(json_base.cam[_cam].datecreated - unix_timestamp) <= (seconds_week))
		js_forceplaymobile = true;

	// special cases
	if ( "1" == cam_state &&   (js_allowmobile=="true" || js_forceplaymobile==true || camData.group_id =="statueofliberty" || camData.group_id =="glendale" || camData.group_id =="warhol" || camData.group_id =="fishbowl_2015" || camData.group_id =="fishbowl_2014" || camData.group_id =="aprilfools2014" ||  camData.group_id =="sandyhook" )  ) 
	// && typeof getCookie("warholQR") != "undefined"))
	{	
		if (_cam == "liberty_torch" )
		{
			sWidth			= "960";
			sHeight			= "540";
			basic_html	= '<div id="ecvrDIV" style="width:'+sWidth+'; height:'+sHeight+'px;"></div>';
			$j("#ECNglobalContainer").css("height",sHeight+'px');
			flashContainer.html(loading);
			flashContainer.html(basic_html);
			ecvr.init();
			return(1);
		}

		// all the rest
		if (camData.live_type == "mega_hd" ||  camData.live_type == "flashvideo")
		{
			sWidth			= "960";
			sHeight			= "540";
			var sPath		= camData.livestreamingpath;//livestreamingpath
			var sFile;
			
			// regex for myec mobile:  regex ="/.*/(.*?\.flv)"
			// json_base.cam.shortname.livestreamingpath.match(regex);
			
			if  ("myec" == template_mode)
			{
				var regex		= "/.*/(.*?\.flv)";
				var rmatch		= json_base.cam[shortname].livestreamingpath.match(regex);
				sFile			= rmatch[1];
				
			}
			else if (camData.cdn_status == "true")
				sFile			= sPath.substr(8) + ".flv";
			else
				sFile			= sPath.substr(12); //  /fecnetwork/libertyHD1.flv
			
			//console.log(sPath);
			//console.log(sFile);
			var frameSrc	= '/cams/includes/player_viewer_f.php?template_mode='+template_mode+'&inet='+camData.inet+'&type=h264&vid='+sFile+'&w='+sWidth+'&h='+sHeight;
			
			basic_html 		= '<iframe width="'+sWidth+'" height="'+sHeight+'" frameborder="0" marginheight="0" marginwidth="0" scrolling="no" src="'+frameSrc+'"></iframe>';
		}
		else
		{
			sWidth			= "960";
			sHeight			= "540";
			var timestamp	= new Date().getTime();
			var iPath;
			
			// bandaid for the liberty popart cam
			if (_cam == "liberty_popart")
				iPath		= "http://www.earthcam.com/cams/includes/image.php?img=bc8cfbf74da8fdd0156a05eaf7edfa82" + "&t=" + timestamp;
			else
				iPath		= camData.imagedomain + camData.livepath + "?"+ timestamp;

			basic_html		= '<div id="basic_loading"><img src="'+iPath+'" width="'+sWidth+'" height="'+sHeight+'"  alt="" title="" /></div>';
		}
	}
	else // regular case
	{
		sWidth			= "960";
		sHeight			= "540";
		var timestamp	= new Date().getTime();
		var iPath;
		
		if ("0" == cam_state)
		{
			iPath		= offlineimage;
			//console.warn("cam state is 0 logic");
		}
		else if (camData.live_type == "megapixel")
			iPath		= camData.imagedomain + camData.livepath + "?"+ timestamp;
		else
			iPath		= "http://www.earthcam.com/cams/includes/image.php?img=" + camData.inet + "&t="+ timestamp;

		basic_html		= '<div id="basic_loading"><img src="'+iPath+'" width="'+sWidth+'" height="'+sHeight+'"  alt="" title="" /></div>';
		
		//console.warn(offlineimage);
		//console.warn(cam_state);
		//console.warn(iPath);
	}

	flashContainer.html(basic_html);
	
		
	
	return(1);
}

function _startPlayer()
{
	//return false;
		/*
			Flashvars setup
		*/
		
		/* event mode */
		if ("newyears" == js_event_mode)
		{
			flashvars.newYear = 2016; //the year that the relevent new years day is in.
			flashvars.midnightTime = 1451624400*1000; //2015-01-01 00:00:00 -05:00
			flashvars.broadcastStartTime = 1451617200*1000; // 2015-12-31 22:00:00 -05:00
			//flashvars.broadcastEndTime = 1451626200*1000; // 2016-01-01 00:30:00 -05:00
			flashvars.broadcastEndTime = 1451625300*1000; // 2016-01-01 00:15:00 -05:00
			
			//flashvars.disableCameraLikes = true;
		}
		
		if (template_mode == "ecn")
		{
			js_defaultCam = getQueryVariable('cam');
			//console.log(js_defaultCam);
			
			// determine if hash tag history fallback is present, for IE <=9 and compat mode
			var hash_cam = loadCameraViaHash();
			
			if (hash_cam !== false)
			{
				//console.warn('hash_cam not false');
				flashvars.cam			= hash_cam;
			}
			else
			{
				var valid_cam = jQuery.inArray(js_defaultCam, js_cam_list);
				
				if ( js_defaultCam == undefined)
				{
					js_defaultCam = 'tower1cu';
				}
				else if (valid_cam == -1)
				{
					js_defaultCam = 'gzmp';
				}
			
				flashvars.cam			= js_defaultCam;
				//flashvars.cam			= "tower1cu";
			}
		}
		else
		{
			js_defaultCam			= 'tower1cu';
			flashvars.cam			= 'tower1cu';
			
		}
		
		var base_url_full		= History.getState().hashedUrl.split("?");
		flashvars.base_url 		= base_url_full[0];
		//console.log(base_url);
		
		//flashvars.timestamp		= "";	
		flashvars.hidebanner	= "";
		flashvars.json			= json_base;
		flashvars.userid		= js_userid;
		flashvars.camNameLookup = camNameLookup;
		flashvars.path			= "/cams/newyork/groundzero//";
		flashvars.needWaterTemp = "";
		flashvars.showplaceholder	= "false";
		flashvars.ecnBlack		= "";
		flashvars.streamTimeOut	= '30';flashvars.ads = 'false';		flashvars.disableCameraLikes 		= false;
		flashvars.thumbLabel				= '';
		flashvars.thumbLabelRight			= '';
		flashvars.thumbLabel2				= '';
		flashvars.eventMode					= '';
		flashvars.playerPathState			= "com";
		
				
		//flashvars.startOnWorldTour			= ; //Start the player on the world tour? (true,false)
		//flashvars.displayWorldTourGoogleAds = true;  //Show google ads on world tour button?(true,false)

		flashvars.ectv_mode					= template_mode;

		flashvars.swf				= "http://static.earthcamcdn.com/swf/ectv/ectv.swf?20160316";
		flashvars.videoPlayer		= "http://static.earthcamcdn.com/swf/streaming/stream_viewer_v3.swf?20160316";
		flashvars.pano_swf			= "http://static.earthcamcdn.com/swf/pano/dot_com_pano.swf";
		flashvars.ecvr_swf			= "http://static.earthcamcdn.com/swf/ecvr/ecvr_player.swf";
		flashvars.ecvr_library		= "http://static.earthcamcdn.com/swf/ecvr/ecvr_library_1_18.swf";

		//flashvars.swf				= "/swf/ectv/ectv.swf?20160316";
		//flashvars.videoPlayer		= "/swf/streaming/stream_viewer_v3.swf?20160316";
		
		/*
			PLAYER INIT SPECIFIC
		*/
		//console.time("Runtime"); //TESTING
		ecnPlayer = new earthcam.ECNPlayer(jQuery("#ecnPlayer"));
		ecnPlayer.init(flashvars);
		//console.timeEnd("Runtime"); //TESTING
		
		}

-->
</script>


</head>

